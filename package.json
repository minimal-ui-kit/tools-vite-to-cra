{
  "name": "@minimals/vite-to-cra",
  "version": "1.0.2",
  "description": "Migrate your Vite.js projects to CRA for Minimal UI",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minimal-ui-kit/tools-vite-to-cra.git"
  },
  "author": "Minimals",
  "license": "MIT",
  "keywords": [
    "Codemod",
    "Vite",
    "CRA",
    "Migration"
  ],
  "scripts": {
    "bundle-packages": "tsup",
    "bundle-packages:watch": "tsup --watch",
    "pre-publish": "npm run bundle-packages && node scripts/pre-publish",
    "publish-package": "npm run pre-publish && cd ./dist && npm publish",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "devDependencies": {
    "@types/prettier": "^3.0.0",
    "@types/recursive-readdir": "^2.2.3",
    "tsup": "^7.2.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "enquirer": "^2.4.1",
    "recursive-readdir": "^2.2.3"
  },
  "bin": {
    "vite-to-cra": "index.js"
  },
  "bugs": {
    "url": "https://github.com/minimal-ui-kit/tools-vite-to-cra/issues"
  },
  "homepage": "https://github.com/minimal-ui-kit/tools-vite-to-cra#readme"
}
